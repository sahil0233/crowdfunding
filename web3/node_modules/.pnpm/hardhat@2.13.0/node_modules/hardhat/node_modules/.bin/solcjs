#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/yourc/Desktop/pullfromgithub/project_crowdfunding/web3/node_modules/.pnpm/solc@0.7.3_debug@4.3.4/node_modules/solc/node_modules:/mnt/c/Users/yourc/Desktop/pullfromgithub/project_crowdfunding/web3/node_modules/.pnpm/solc@0.7.3_debug@4.3.4/node_modules:/mnt/c/Users/yourc/Desktop/pullfromgithub/project_crowdfunding/web3/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/yourc/Desktop/pullfromgithub/project_crowdfunding/web3/node_modules/.pnpm/solc@0.7.3_debug@4.3.4/node_modules/solc/node_modules:/mnt/c/Users/yourc/Desktop/pullfromgithub/project_crowdfunding/web3/node_modules/.pnpm/solc@0.7.3_debug@4.3.4/node_modules:/mnt/c/Users/yourc/Desktop/pullfromgithub/project_crowdfunding/web3/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../solc@0.7.3_debug@4.3.4/node_modules/solc/solcjs" "$@"
else
  exec node  "$basedir/../../../../../solc@0.7.3_debug@4.3.4/node_modules/solc/solcjs" "$@"
fi
